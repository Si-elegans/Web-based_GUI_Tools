<div class="row">
    <div class="col-sm-12">
        <div class=" title">Experiment type</div>
    </div>
</div>
<div class="row">
    <div class="col-sm-12 block"> 
        <div class="btn-group">
            <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                {{typeName}}<span class="caret"></span>
            </button>
            <ul class="dropdown-menu" role="menu">
                <li id="chemoOsmotic" class="btn">Osmotic Ring</li>
				<li id="chemoQuadrants1" class="btn">1 Chemotaxis Quadrant</li>
				<li id="chemoQuadrants2" class="btn">2 Chemotaxis Quadrants</li>
				<li id="chemoQuadrants4" class="btn">4 Chemotaxis Quadrants</li>
				<li id="staticPointSource" class="btn">Static Point Source</li>
            </ul>
        </div>
    </div>
</div>
{{#if isOsmotic}}
{{render "osmoticRing" osmoticRing}}
{{/if}}
{{#if isQuadrants1}}
{{render "chemotaxisQuadrants1" chemotaxisQuadrants1}}
{{/if}}
{{#if isQuadrants2}}
{{render "chemotaxisQuadrants2" chemotaxisQuadrants2}}
{{/if}}
{{#if isQuadrants4}}
{{render "chemotaxisQuadrants4" chemotaxisQuadrants4}}
{{/if}}
{{#if isStatic}}
{{render "staticPointSource" staticPointSourceConf}}
{{/if}}